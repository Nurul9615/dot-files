{
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[python]": {
    "editor.formatOnType": true,
    "editor.defaultFormatter": "ms-python.black-formatter"
  },
  "[terraform]": {
    "editor.defaultFormatter": "hashicorp.terraform"
  },
  "editor.bracketPairColorization.enabled": true,
  "editor.codeActionsOnSave": {
    "source.organizeImports": "explicit"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.guides.bracketPairs": true,
  "editor.minimap.autohide": true,
  "editor.minimap.enabled": false,
  "editor.semanticHighlighting.enabled": true,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "git.autofetch": false,
  "gitlens.codeLens.enabled": false,
  "gopls": {
    "ui.semanticTokens": true
  },
  "indentRainbow.colors": [
    "rgba(255,255,64,0.07)",
    "rgba(127,255,127,0.07)",
    "rgba(255,127,255,0.07)",
    "rgba(79,236,236,0.07)"
  ],
  "indentRainbow.indicatorStyle": "light",
  "python.analysis.extraPaths": [],
  "python.analysis.typeCheckingMode": "basic",
  "redhat.telemetry.enabled": false,
  "security.workspace.trust.untrustedFiles": "open",
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "terminal.integrated.minimumContrastRatio": 1,
  "terminal.integrated.profiles.windows": {
    "Command Prompt": {
      "args": [],
      "icon": "terminal-cmd",
      "path": ["${env:windir}\\Sysnative\\cmd.exe", "${env:windir}\\System32\\cmd.exe"]
    },
    "Git Bash": {
      "source": "Git Bash"
    },
    "PowerShell": {
      "icon": "terminal-powershell",
      "source": "PowerShell"
    }
  },
  "terminal.integrated.scrollback": 5000,
  "terminal.integrated.tabs.defaultColor": "terminal.ansiGreen",
  "window.commandCenter": false,
  "window.titleBarStyle": "custom",
  "workbench.editor.empty.hint": "hidden",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.layoutControl.enabled": false,
  "workbench.startupEditor": "none",
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[dockerfile]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[ignore]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[properties]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[hosts]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "editor.wordWrap": "on",
  "emmet.includeLanguages": { "jinja-html": "html", "django-html": "html" },
  "material-icon-theme.languages.associations": {
    "jinja-html": "html"
  },
  "files.associations": {
    "*.html": "jinja-html"
  },
  "files.autoSave": "afterDelay",
  "prettier.printWidth": 125,
  "gitlab.duoCodeSuggestions.enabled": true,
  "gitlab.duoCodeSuggestions.preferredAccount": "Nurul1596",
  "workbench.colorTheme": "Default Dark+",
  "gitlens.graph.minimap.enabled": false,
  "aws.telemetry": false,
  "terminal.external.osxExec": "iTerm.app",
  "github.copilot.enable": {
    "*": false,
    "plaintext": false,
    "markdown": false,
    "scminput": false
  },
  "python.locator": "js",
  "docker.extension.enableComposeLanguageServer": false
}
